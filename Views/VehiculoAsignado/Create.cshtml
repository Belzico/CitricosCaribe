@model CitricosCaribe.Models.VehiculoAsignado

@{
    ViewData["Title"] = "Create";
}

<h1>Asignar Vehiculo</h1>

<h4></h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TrabajadorID" class="control-label"></label>
                <select asp-for="TrabajadorID" class ="form-control" asp-items="ViewBag.TrabajadorID"></select>
            </div>
            <div class="form-group">
                <label asp-for="VehiculoID" class="control-label"></label>
                <select asp-for="VehiculoID" class ="form-control" asp-items="ViewBag.VehiculoID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
